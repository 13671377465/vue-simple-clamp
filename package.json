    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-buble": "^0.19.6",
